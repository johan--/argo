<h2>Workflow: <%= workflow_name %></h2>
<table class='table table-striped table-hover table-condensed workflow-grid-table'>
  <tbody>
    <tr>
      <th class="process" colspan="2">Process</th>
      <th class="description">Description</th>
      <th class="count waiting info">Waiting</th>
      <th class="count ready warning">Ready</th>
      <th class="count queued warning">Queued</th>
      <th class="count errors danger">Error</th>
      <th class="count skipped success">Skipped</th>
      <th class="count completed success">Completed</th>
    </tr>
    <%= render(
          partial: 'workflow_table_process',
          collection: workflow_process_names,
          as: :process_info,
          locals: { wf_data: data, wf_name: name }
        ) %>
  </tbody>
</table>
<hr>
